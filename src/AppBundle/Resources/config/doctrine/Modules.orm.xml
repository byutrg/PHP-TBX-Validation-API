<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Modules" table="modules">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="64" nullable="false"/>
    <field name="definition" type="text" column="definition" length="65535" nullable="false"/>
    <field name="rng" type="text" column="rng" length="65535" nullable="false"/>
    <field name="sch" type="text" column="sch" length="65535" nullable="false"/>
    <field name="tbxmd" type="text" column="tbxmd" length="65535" nullable="false"/>
  </entity>
</doctrine-mapping>
